{
  "Version": "2012-10-17",
  "Statement": [{
    "Effect": "Allow",
    "Principal": { "Federated": "arn:aws:iam::685801731369:oidc-provider/token.actions.githubusercontent.com" },
    "Action": "sts:AssumeRoleWithWebIdentity",
    "Condition": {
      "StringEquals": {
        "token.actions.githubusercontent.com:aud": "sts.amazonaws.com"
      },
      "StringLike": {
        "token.actions.githubusercontent.com:sub": [
          "repo:jimmy23782/infraRepo:ref:refs/heads/main",
          "repo:jimmy23782/infraRepo:ref:refs/heads/feature/*",
          "repo:jimmy23782/infraRepo:pull_request"
        ]
      }
    }
  }]
}